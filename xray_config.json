        "wsSettings": {
          "path": "/vmess",
          "headers": {
            "Host": "yourdomain.com"
          }
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": ["http", "tls"]
      }
    },
    {
      "port": 445,
      "protocol": "trojan",
      "tag": "trojan-ws-tls",
      "settings": {
        "clients": [],
        "fallbacks": [
          {
            "dest": 80
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
